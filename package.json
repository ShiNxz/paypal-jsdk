{
	"name": "paypal-jsdk",
	"version": "0.0.55",
	"description": "A Node.js wrapper for PayPal payments and subscriptions.",
	"main": "build/index.mjs",
	"module": "build/index.esm",
	"types": "build/index.d.ts",
