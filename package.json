		"axios": "^1.4.0",
		"zod": "^3.21.4"
	},
	"devDependencies": {
		"@types/node": "^20.4.2",
		"dotenv": "^16.3.1",
		"tsup": "^7.1.0",
		"tsx": "^4.7.0",
